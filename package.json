{
	"name": "md-live",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"server": "nodemon server.js",
		"dev": "concurrently \"npm run server\" \"npm run next\"",
		"next": "next dev",
		"build": "next build",
		"start": "next start",
		"format": "prettier \"src/**/*.{js,jsx,json}\" --write",
		"lint": "eslint . \"**/*.{js,jsx,ts,tsx}\""
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		],
		"*.{js,jsx,ts,tsx,json}": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@material-ui/core": "^4.9.9",
		"@material-ui/icons": "^4.9.1",
		"concurrently": "^5.1.0",
		"express": "^4.17.1",
		"mongoose": "^5.9.7",
		"next": "^9.3.4",
		"nodemon": "^2.0.2",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-markdown": "^4.3.1"
	},
	"devDependencies": {
		"babel-plugin-import": "^1.13.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.1.3",
		"prettier": "^2.0.4"
	}
}
